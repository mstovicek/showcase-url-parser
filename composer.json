{
    "name": "milan.stovicek/url-parser",
    "description": "URL parser",
    "autoload": {
        "psr-4": {
            "Parser\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-0": {
            "Parser\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": [
            "php vendor/bin/phpunit"
        ]
    },
    "require": {
        "symfony/console": "^3.2",
        "symfony/dependency-injection": "^3.2",
        "symfony/config": "^3.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.0",
        "friendsofphp/php-cs-fixer": "1.*"
    },
    "config": {
        "bin-dir": "vendor/bin"
    },
    "authors": [
        {
            "name": "Milan Stovicek",
            "email": "stovicek.milan@gmail.com"
        }
    ]
}
